#include <iostream>
using namespace std;

int main(){
  int n ;
  cin>>n;
     
    for (int i= 1; i<=10; i++){
       for (int j=1;j<=1;j++){
           cout << n;
       }
        for (int k=1;k<=1;k++){
            cout <<"*";
        }
         for (int l=1;l<=1;l++){
           cout << i <<" = ";
       }
        for (int m=1;m<=1;m++){
            cout <<i*n;
        }
     cout <<endl;
    }
    
}
