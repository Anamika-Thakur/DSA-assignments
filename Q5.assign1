#include<iostream>
using namespace std;

int countDigits(long long n) 
{ 

    if (n == 0) 

        return 1; 

    int count = 0; 

    while (n != 0) { 

        n = n /10; 

        ++count; 

    } 

    return count; 
} 


int main(){

long long n;

cout << "enter the number" << endl;

cin>>n;

cout << "Number of digits : " << countDigits(n); 

}
